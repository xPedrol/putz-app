{
  "name": "putz-app",
  "version": "1.6.0",
  "scripts": {
    "ng": "ng",

    "start": "ng serve",
    "start-local": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --aot",
    "build-dev": "ng build --aot --configuration=development",
    "b": "npm run build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run putz-app:serve-ssr",
    "serve:ssr": "node dist/putz-app/server/main.js",
    "build:ssr": "npm run build && ng run putz-app:server",
    "build-dev:ssr": "npm run build-dev && ng run putz-app:server --configuration=development",
    "prerender": "ng run putz-app:prerender",
    "source-map-explorer": "source-map-explorer dist/putz-app/browser/*.js",
    "dev-c:ssr": "ng run customer-pages:serve-ssr",
    "serve-c:ssr": "node dist/customer-pages/server/main.js",
    "build-c:ssr": "npm run build-c && ng run customer-pages:server",
    "prerender-c": "ng run customer-pages:prerender",
    "start-c": "ng serve --port=4201 --project=customer-pages",
    "build-c": "ng build --aot --project=customer-pages",
    "build-dev-c": "ng build --aot --configuration=development --project=customer-pages",
    "build-dev-c:ssr": "npm run build-dev-c && ng run customer-pages:server --configuration=development",
    "source-map-explorer-c": "source-map-explorer dist/customer-pages/browser/*.js",
    "dev-startup:ssr": "ng run startup-site:serve-ssr",
    "serve-startup:ssr": "node dist/startup-site/server/main.js",
    "build-startup:ssr": "npm run build-startup && ng run startup-site:server",
    "prerender-startup": "ng run startup-site:prerender",
    "start-startup": "ng serve --port=4201 --project=startup-site",
    "build-startup": "ng build --aot --project=startup-site",
    "build-dev-startup": "ng build --aot --configuration=development --project=startup-site",
    "build-dev-startup:ssr": "npm run build-dev-startup && ng run startup-site:server --configuration=development",
    "source-map-explorer-startup": "source-map-explorer dist/startup-site/browser/*.js",
    "gen-build:prod": "node scripts/cleanDist.js && npm run build:ssr && npm run build-c:ssr && npm run build-startup:ssr && cp -r dist/* build",
    "gen-build-main:prod": "node scripts/cleanDist.js && npm run build:ssr && cp -r dist/* build",
    "gen-build:dev": "node scripts/cleanDist.js && npm run build-dev:ssr && npm run build-dev-c:ssr  && npm run build-dev-startup:ssr && cp -r dist/* build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/forms": "^13.3.11",
    "@angular/localize": "^13.3.11",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/platform-server": "^13.3.11",
    "@angular/router": "^13.3.11",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@ckeditor/ckeditor5-build-classic": "^32.0.0",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@nebular/auth": "^9.0.1",
    "@nebular/eva-icons": "^9.0.1",
    "@nebular/moment": "^9.0.1",
    "@nebular/security": "^9.0.1",
    "@nebular/theme": "^9.0.1",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "@nguniversal/express-engine": "^13.0.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@popperjs/core": "^2.10.2",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.8.0",
    "echarts": "^5.2.2",
    "eva-icons": "^1.1.3",
    "express": "^4.15.2",
    "lodash": "^4.17.21",
    "marked": "^4.0.17",
    "moment": "^2.29.1",
    "nebular-icons": "1.1.0",
    "ng2-charts": "^3.0.11",
    "ng2-sticky-nav": "^10.0.0",
    "ngx-accordion": "^0.0.17",
    "ngx-currency": "^2.5.3",
    "ngx-dropzone": "^3.0.0",
    "ngx-echarts": "^8.0.1",
    "ngx-image-cropper": "^6.2.1",
    "ngx-mask": "^13.1.1",
    "ngx-owl-carousel-o": "^7.0.2",
    "ngx-papaparse": "^5.0.0",
    "ngx-scrolltop": "^4.1.2",
    "ngx-smart-modal": "^7.4.1",
    "ngx-tabset": "^2.2.0",
    "ngx-webstorage": "^9.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/localize": "^13.3.11",
    "@nguniversal/builders": "^13.0.1",
    "@schematics/angular": "~13.1.3",
    "@types/chart.js": "^2.9.37",
    "@types/ckeditor__ckeditor5-build-classic": "^29.0.1",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.10.3",
    "@types/lodash": "^4.14.180",
    "@types/node": "^12.20.38",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.0.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "~4.5.2"
  }
}
